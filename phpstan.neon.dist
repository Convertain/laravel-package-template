parameters:
    level: 10

    paths:
        - src
        - tests

    tmpDir: build/phpstan

    checkMissingIterableValueType: true
    checkGenericClassInNonGenericObjectType: true

    treatPhpDocTypesAsCertain: true
    reportUnmatchedIgnoredErrors: true

    ignoreErrors:
        - '#Unsafe usage of new static#'
